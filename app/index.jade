doctype html5

html(lang="en")
  head
    title KIOS Performance Dashboard
    link(rel="stylesheet", href="/components/normalize-css/normalize.css")
    link(rel="stylesheet", href="/css/main.css")
    link(rel="stylesheet", href="/css/style.css")
  body

    .dashboard
      .history-length
        .title Number of builds:
        input(value=5)
        button(name="history-refresh-button") Go
      .update-status
        .progress.hidden
          img(src="/images/ajax-loader.gif")
        button.update Update now

      .note
        a(href="https://knowledge.solita.fi/display/MMLYP/Perffitulosten+hallinta+ja+raportointi",
          target="_blank") Ohje tulosten hallintaan

      .test-case-container
        .title Lainhuutotodistukset
        svg.graph.tulosteet.lh.response-time
        svg.graph.tulosteet.lh.response-time-scatter-plot

      .test-case-container
        .title Rasitustodistukset
        svg.graph.tulosteet.rt.response-time
        svg.graph.tulosteet.rt.response-time-scatter-plot

      .test-case-container
        .title Vuokraoikeustodistukset
        svg.graph.tulosteet.vo.response-time
        svg.graph.tulosteet.vo.response-time-scatter-plot

      .test-case-container
        .title Lainhuutorekisteriote
        svg.graph.tulosteet.lhro.response-time
        svg.graph.tulosteet.lhro.response-time-scatter-plot

      .note
         a(href="http://svn.solita.fi/svns/kios/application/trunk/kios-tp-eraajo-velocity-performance/scripts/run-tests.sh",
           target="_blank") Urlit throughput-testeille

      .test-case-container
        .title Eräajot
        svg.graph.eraajo.tietopalvelu.throughput
        .eraajo.tietopalvelu.status
          .thead
            span.title.stroke
            span.title.testCaseId
            span.build Build
            span.title.throughput T [1/s]
            span.title.count N
          .tbody
            .row
              span.stroke
              span.testCaseId
              span.build
              span.throughput
              span.count

      .test-case-container
        .title Kyselypalvelu
        svg.graph.kyselypalvelu.tietopalvelu.throughput
        .kyselypalvelu.tietopalvelu.status
          .thead
            span.title.stroke
            span.title.testCaseId
            span.build Build
