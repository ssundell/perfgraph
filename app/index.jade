      .test-case-container
        .title BI-raportit
          span &nbsp;(&nbsp;
            a(href='http://murky.solita.fi:8080/job/KIRRE%20BI%20Nightly/') 'KIRRE BI Nightly'
          span -buildin kesto )
        img.graph.bi-raportit

